# -----------------------------------Makefile------------------------------------
NAME	= so_long

DEBUG	= debug.out
# -----------------------------------Sources-------------------------------------

HEADER	= includes/so_long.h

SRCS	= main.c \
		  map_parsing.c \
		  map_utils.c \
		  map_checker.c \
		  map_checker_utils.c \
		  game_init.c \
		  game_utils.c \
		  put_items.c \
		  sprites_init.c

SRC		= ${addprefix sources/, ${SRCS}}

OBJS	= ${SRC:.c=.o}

# -----------------------------------Compilation---------------------------------

CC			= cc

FLAGS		= -Wall -Wextra -Werror -O3 -march=native

MLX_FLAGS	= -lmlx -framework OpenGL -framework AppKit

RM			= rm -f

FSANITIZE	= -fsanitize=address -g

# -----------------------------------Libraries-----------------------------------

FT_FOLDER	= libft

FT			= ${FT_FOLDER}/libft.a

MAKE_FT		= make -s -C ${FT_FOLDER}

INCLUDES	= -I ${FT_FOLDER}/includes \
			  -I ./includes \
			  -I/opt/X11/include -Imlx

LIBRARIES	= -L./${FT_FOLDER} -lft

# -----------------------------------Rules---------------------------------------

all: ${NAME}

${NAME}: ${FT} ${OBJS} ${HEADER}
		${CC} ${FLAGS} ${OBJS} ${LIBRARIES} -o ${NAME} ${MLX_FLAGS} 

${FT}:
	${MAKE_FT}

%.o: %.c
	${CC} ${FLAGS} -c ${INCLUDES} $< -o $@

debug: ${FT} ${OBJS} ${HEADER}
	${CC} ${FLAGS} ${FSANITIZE} ${OBJS} ${LIBRARIES} -o ${DEBUG} ${MLX_FLAGS}

re: fclean all

clean:
		@${RM} ${OBJS}
		@${MAKE_FT} clean
		@echo "It's clean."

fclean: clean
		@${RM} ${NAME}
		@${RM} ${DEBUG}
		@${MAKE_FT} fclean
		@echo "It's like mega clean mate."

# -----------------------------------Fun_Rules----------------------------------

